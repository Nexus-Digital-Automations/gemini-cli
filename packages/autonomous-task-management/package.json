{
  "name": "@google/autonomous-task-management",
  "version": "1.0.0",
  "description": "Autonomous Task Management System - Testing Infrastructure",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "test:performance": "vitest run --config vitest.performance.config.ts",
    "test:all": "npm run test && npm run test:integration && npm run test:e2e && npm run test:performance",
    "build": "tsc",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix"
  },
  "dependencies": {
    "@google/test-utils": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^3.1.1",
    "typescript": "^5.0.0",
    "vitest": "^3.2.4",
    "mock-fs": "^5.5.0",
    "memfs": "^4.42.0"
  },
  "files": [
    "dist/",
    "src/"
  ],
  "keywords": [
    "autonomous",
    "task-management",
    "testing",
    "gemini-cli",
    "vitest"
  ],
  "author": "Google Gemini CLI Team",
  "license": "Apache-2.0"
}